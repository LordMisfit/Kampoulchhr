			}
			CRG2 H 9
			{
				A_FaceTarget();
				user_shotaccuracy = 11.25; // 22.5;
				user_shotzaccuracy = 0.0;
				if (kc_monsterlevelenabledflags && CountInv("MonsterLevel") > 100) { user_shotaccuracy = user_shotaccuracy * 5 / 8; user_shotzaccuracy = user_shotzaccuracy * 3 / 8; }
