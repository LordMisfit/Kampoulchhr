			double ArmorWeight;
			if (owner.CountInv("TSArmorTier2"))
			{
				if (A_CheckForHeiwa())
				{
					ArmorWeight = 0.015625;
					if (berserked) ArmorWeight *= 0.25;
				}
				else
				{
					ArmorWeight = 0.0625;
					if (berserked) ArmorWeight *= 0.875;
				}
				ArmorWeight *= armordiffmod;
				ArmorEnc = ArmorWeight;
			}
			else
			if (owner.CountInv("TSArmorTier1"))
			{
				if (A_CheckForHeiwa())
				{
					ArmorWeight = 0.0125;
					if (berserked) ArmorWeight *= 0.25;
				}
				else
				{
					ArmorWeight = 0.05;
					if (berserked) ArmorWeight *= 0.875;
				}
				ArmorWeight *= armordiffmod;
				ArmorEnc = ArmorWeight;
			}
			else
			{
				if (A_CheckForHeiwa())
				{
					if (darmortype == 1) // Green Armor [Doom-type] (100% of HP max)
					{
						ArmorWeight = 0.25;
						if (berserked) ArmorWeight *= 0.4;
					}
					if (darmortype == 2) // Blue Armor [Doom-type] (200% of HP max)
					{
						ArmorWeight = 0.20;
						if (berserked) ArmorWeight *= 0.4;
					}
				}
				else
				{
					if (darmortype == 1) // Green Armor [Doom-type] (100% of HP max)
					{
						ArmorWeight = 0.1875;
						if (berserked) ArmorWeight *= 0.6;
					}
					if (darmortype == 2) // Blue Armor [Doom-type] (200% of HP max)
					{
						ArmorWeight = 0.15;
						if (berserked) ArmorWeight *= 0.6;
					}
				}
				ArmorWeight *= armordiffmod;
				ArmorEnc = (ArmorWeight * ArmorRatio);
			}
			if (kcdebug_jumpingcalcs) Console.Printf("ArmorEnc: %.8f, ArmorWeight: %.8f, ArmorRatio: %.8f (%d / %d)", ArmorEnc, ArmorWeight, ArmorRatio, ArmorAmt, BMaxArmor);
			ArmorEnc *= kc_armorencumbrance;
			if (owner.waterlevel >= 3) ArmorEnc *= 1.25;
			
			double lvlfact = 0.02;
			if (skill <= 0) lvlfact = 0.04;
			if (skill == 1) lvlfact = 0.035;
			if (skill == 2) lvlfact = 0.03;
			if (skill == 3) lvlfact = 0.025;
			if (skill >= 4) lvlfact = 0.02;
			if (A_CheckForHeiwa()) lvlfact *= 1.75;
			if (owner.waterlevel >= 3) lvlfact *= 0.75;
			
			if (ArmorEnc > 0.0) ArmorEnc *= (1.0 - (ActualLevel * lvlfact));
			if (!owner.CountInv("KCPowerKampoul")) 
			{
				ArmorJumpZ -= ArmorEnc;
				if (kcdebug_jumpingcalcs) Console.Printf("JumpZ [Armor]: %.8f, ArmorEnc: %.8f, kc_armorencumbrance: %.8f", ArmorJumpZ, ArmorEnc, kc_armorencumbrance);
			}

			// Special
			double SpecialJumpZ = 1.00;
			if (A_CheckForHeiwa())
			{
				if (owner.CountInv("TSArmorTier2")) 
				{
					if (owner.CountInv("TimeSeraphVisor")) SpecialJumpZ *= frandompick(1.0,1.1875,1.1875,1.1875,1.375); 
																						else SpecialJumpZ *= frandompick(1.0,1.125,1.125,1.125,1.25);
				}
			}
			if (kcdebug_jumpingcalcs) Console.Printf("JumpZ [SpecialItems]: %.8f", SpecialJumpZ);

			// Weapon Checks
			weapon = owner.player.ReadyWeapon;
			double WeaponJumpZ = 1.00;
			double WeaponJumpEx = 0.00;
			if (weapon is "FistMarine" || 
					weapon is "FistHeiwa" ||
					weapon is "KnifeHeiwa")
			{
				if (berserked)
				{
					if (A_CheckForHeiwa()) WeaponJumpEx += 0.20;
														else WeaponJumpEx += 0.1625;
				}
				else 
				{
					WeaponJumpEx += 0.125;
				}
			}
			if (weapon is "RiotGun")
			{
				if (A_CheckForHeiwa())
				{
					if (berserked) WeaponJumpEx -= 0.025;
										else WeaponJumpEx -= 0.1;
				}
				else
				{
					if (berserked) WeaponJumpEx -= 0.0125;
										else WeaponJumpEx -= 0.05;
				}
			}
			if (weapon is "TommyGun")
			{
				bool dualtommys = false;
				if (owner.CountInv("DualTommyGuns") && !owner.CountInv("DualTommyGunsDisabled")) dualtommys = true;
				
				if (A_CheckForHeiwa())
				{
					if (dualtommys)
					{
						if (berserked) WeaponJumpEx -= 0.028125;
											else WeaponJumpEx -= 0.1125;
					}
					else
					{
						if (berserked) WeaponJumpEx -= 0.0125;
											else WeaponJumpEx -= 0.05;
					}
				}
				else
				{
					if (dualtommys)
					{
						if (berserked) WeaponJumpEx -= 0.0140625;
											else WeaponJumpEx -= 0.05625;
					}
					else
					{
						if (berserked) WeaponJumpEx -= 0.00625;
											else WeaponJumpEx -= 0.025;
					}
				}
			}
			if (weapon is "BlasterRifle")
			{
				if (A_CheckForHeiwa())
				{
					if (berserked) WeaponJumpEx -= 0.0125;
										else WeaponJumpEx -= 0.05;
				}
				else
				{
					if (berserked) WeaponJumpEx -= 0.00625;
										else WeaponJumpEx -= 0.025;
				}
			}
			if (weapon is "Deviation")
			{
				if (A_CheckForHeiwa())
				{
					if (berserked) WeaponJumpEx -= 0.0375;
										else WeaponJumpEx -= 0.15;
				}
				else
				{
					if (berserked) WeaponJumpEx -= 0.025;
										else WeaponJumpEx -= 0.10;
				}
			}
			if (weapon is "Devastator")
			{
				bool dualdevs = false;
				if (owner.CountInv("DualDevastators") && !owner.CountInv("DualDevastatorsDisabled")) dualdevs = true;

				if (A_CheckForHeiwa())
				{
					if (dualdevs)
					{
						if (berserked) WeaponJumpEx -= 0.05625;
											else WeaponJumpEx -= 0.225;
					}
					else
					{
						if (berserked) WeaponJumpEx -= 0.025;
											else WeaponJumpEx -= 0.1;
					}
				}
				else
				{
					if (dualdevs)
					{
						if (berserked) WeaponJumpEx -= 0.028125;
											else WeaponJumpEx -= 0.1125;
					}
					else
					{
						if (berserked) WeaponJumpEx -= 0.0125;
											else WeaponJumpEx -= 0.05;
					}
				}
			}
			if (weapon is "TeslaCannon")
			{
				bool dualteslas = false;
				if (owner.CountInv("DualTeslaCannons") && !owner.CountInv("DualTeslaCannonsDisabled")) dualteslas = true;

				if (A_CheckForHeiwa())
				{
					if (dualteslas)
					{
						if (berserked) WeaponJumpEx -= 0.025;
											else WeaponJumpEx -= 0.1;
					}
					else
					{
						if (berserked) WeaponJumpEx -= 0.015625;
											else WeaponJumpEx -= 0.0625;
					}
				}
				else
				{
					if (dualteslas)
					{
						if (berserked) WeaponJumpEx -= 0.0125;
											else WeaponJumpEx -= 0.05;
					}
					else
					{
						if (berserked) WeaponJumpEx -= 0.015625;
											else WeaponJumpEx -= 0.0078125;
					}
				}
			}
			if (weapon is "BHGen")
			{
				if (A_CheckForHeiwa())
				{
					if (berserked) WeaponJumpEx -= 0.0625;
										else WeaponJumpEx -= 0.25;
				}
				else
